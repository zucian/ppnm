set terminal png
set output "$@"
set key outside
set tics in
set xlabel "$x$"
set ylabel "$S_i(x)$"
set title "$\cos(x)$ - Quadratic spline"
set output "quadraticSplinePlot.png"
plot "quadraticSplineOutput.txt" using 1:2 with line title "Interpolant" \
,"quadraticSplineOutput.txt" using 1:3 with line title "Interpolant - GSL" \
,"quadraticSplineOutput.txt" using 1:4 with line title "Interpolant - integral" \
,"quadraticSplineOutput.txt" using 1:5 with line title "Interpolant - integral GSL" \
,"quadraticSplineOutput.txt" using 1:6 with line title "Interpolant - derivative" \
,"quadraticSplineOutput.txt" using 1:7 with line title "Interpolant - derivative GSL" \
, "cosData.txt" using 1:2 with points title "Tabulated data"