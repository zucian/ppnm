set terminal png
set title "Sine function and linear interpolant"
set xlabel "x"
set ylabel "y"
set output "linearSplinePlot.png"
plot 'out.sineData.txt' with points pointtype 2 title "Tabulated data"\
, "out.linearSpline.txt" using 1:2 with line title "Interpolant" \
, "out.linearSpline.txt" using 1:3 with line title "Interpolant of integral" \
, "out.linearSpline.txt" using 1:4 with line title "GSL Interpolant" \
, "out.linearSpline.txt" using 1:5 with line title "GSL Interpolant of integral" \