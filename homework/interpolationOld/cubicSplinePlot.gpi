set terminal png
set output "$@"
set key outside
set xlabel "$x$"
set ylabel "$S_i(x)$"
set title "$\cos(x)$ - Cubic spline"
set output "cubicSplinePlot.png"
plot "cubicSplineOutput.txt" using 1:2 with line title "Interpolant" \
,"cubicSplineOutput.txt" using 1:3 with line title "Interpolant - GSL" \
,"cubicSplineOutput.txt" using 1:4 with line title "Interpolant - integral" \
,"cubicSplineOutput.txt" using 1:5 with line title "Interpolant - integral GSL" \
,"cubicSplineOutput.txt" using 1:6 with line title "Interpolant - derivative" \
,"cubicSplineOutput.txt" using 1:7 with line title "Interpolant - derivative GSL" \
, "cosData.txt" using 1:2 with points title "Tabulated data"